<p>All coins</p>

<table class="table table-striped table-dark">
 <thead>
   <tr>
     <th scope="col">Logo</th>
     <th scope="col">Name</th>
     <th scope="col">Open</th>
     <th scope="col">Total Coin supply</th>
   </tr>
 </thead>
 <tbody>
   <% @data["DISPLAY"].each do |k, v| %>
   <% @coins.each do |c| %>
     <tr>
       <td><img src="<%= c.imageurl %>" alt="profile_img" width="30px"></td>
       <td><%= c.fullname %></td>
       <td><%= v["USD"]["OPENDAY"] %></td>
       <td><%= c.totalcoinsupply %></td>
     </tr>
   <% end %>
   <% end %>
 </tbody>
</table>
