<table class="table table-striped table-dark">
 <thead>
   <tr>
     <th scope="col">Logo</th>
     <th scope="col">Name</th>
     <th scope="col">Price</th>
     <th scope="col">Highday</th>
     <th scope="col">Lowday</th>
   </tr>
 </thead>
 <tbody>
    <% @d1["DISPLAY"].each do |k, v| %>
      <tr>
        <td></td>
        <td><%= k %></td>
        <td><%= v['USD']['PRICE'] %></td>
        <td><%= v['USD']['HIGHDAY'] %></td>
        <td><%= v['USD']['LOWDAY'] %></td>
      </tr>
    <% end %>
 </tbody>
</table>
